server:
  port: 8080
spring:
  application:
    name: User
  datasource:
    username: postgres
    url: jdbc:postgresql://127.0.0.1/UserDatabase
    password: twinkle@123
  jpa:
    properties:
      hibernate:
        format_sql: 'true'
    hibernate:
      ddl-auto: update
    show-sql: 'true'
