spring:
  datasource:
    username: postgres
    url: jdbc:postgresql://127.0.0.1/StudentDatabase
    password: 'twinkle@123'
  jpa:
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5Dialect
    show-sql: 'true'
    hibernate:
      ddl-auto: create-drop
server:
  port: '8080'